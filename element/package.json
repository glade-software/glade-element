{
  "name": "@glade-software/glade-annotatable",
  "description": "a component that makes DOM annotatable",
  "version": "4.1.2",
  "svelte": "src/index.ts",
  "type": "module",
  "main": "src/index.js",
  "unpkg": "dist/glade-annotatable.bundled.js",
  "module": "dist/glade-annotatable.bundled.js",
  "files": [
    "dist/glade-annotatable.bundled.js"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "npm-watch build",
    "dev": "concurrently \"npm-watch build\" \"npm run serve\"",
    "serve": "node ../example-pages/serve",
    "prepublishOnly": "PRE_PUBLISH=true npm run build",
    "check": "svelte-check --fail-on-warnings --compiler-warnings missing-custom-element-compile-options:ignore",
    "check:watch": "npm-watch build"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": [
        "svelte",
        "ts"
      ]
    },
    "extensions": [
      "js"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-replace": "^5.0.2",
    "@tsconfig/svelte": "^4.0.1",
    "concurrently": "^7.3.0",
    "express": "^4.18.1",
    "rollup": "^3.21.5",
    "rollup-plugin-svelte": "^7.1.4",
    "rollup-plugin-typescript2": "^0.34.1",
    "svelte": "^3.59.0",
    "svelte-check": "^3.3.1",
    "svelte-preprocess": "^5.0.3",
    "typescript": "^5.0.4"
  },
  "keywords": [
    "glade",
    "glade-element",
    "annotations",
    "typescript",
    "svelte",
    "web-components"
  ],
  "dependencies": {
    "@material/mwc-button": "^0.20.0",
    "@material/mwc-dialog": "^0.20.0",
    "@material/mwc-linear-progress": "^0.25.3",
    "@material/mwc-textarea": "^0.20.0",
    "builtin-modules": "^3.2.0",
    "cohere-js": "^1.0.21",
    "deepmerge": "^4.2.2",
    "estree-walker": "^3.0.1",
    "firebase": "^9.9.2",
    "fsevents": "^2.3.2",
    "function-bind": "^1.1.1",
    "has": "^1.0.3",
    "is-core-module": "^2.10.0",
    "is-module": "^1.0.0",
    "npm-watch": "^0.11.0",
    "path-parse": "^1.0.7",
    "picomatch": "^2.3.1",
    "require-relative": "^0.8.7",
    "resolve": "^1.22.1",
    "rollup-pluginutils": "^2.8.2",
    "sourcemap-codec": "^1.4.8",
    "svelte-lexical": "^0.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/glade-software/glade-element"
  },
  "author": "Matt Johnston",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/glade-software/glade-element/issues"
  },
  "homepage": "https://github.com/glade-software/glade-element#readme"
}
