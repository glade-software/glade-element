{
  "name": "@glade-software/glade-annotatable",
  "svelte": "src/index.ts",
  "type": "module",
  "main": "src/index.js",
  "unpkg": "dist/glade-annotatable.bundled.js",
  "module": "dist/glade-annotatable.bundled.js",
  "files": [
    "dist/glade-annotatable.bundled.js"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:watch": "npm-watch build",
    "dev": "concurrently \"npm-watch build\" \"npm run serve\"",
    "serve": "node ./dev/serve",
    "semantic-release": "semantic-release",
    "prepublishOnly": "PRE_PUBLISH=true npm run build",
    "check": "svelte-check --fail-on-warnings --compiler-warnings missing-custom-element-compile-options:ignore"
  },
  "watch": {
    "build": {
      "patterns": [
        "src"
      ],
      "extensions": [
        "svelte",
        "ts"
      ]
    },
    "extensions": [
      "js"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@tsconfig/svelte": "^1.0.10",
    "concurrently": "^5.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "express": "*",
    "husky": "^4.3.8",
    "rollup": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "semantic-release": "^17.3.7",
    "svelte": "^3.32.1",
    "svelte-check": "^1.1.32",
    "svelte-preprocess": "^4.6.6",
    "typescript": "^4.1.3"
  },
  "keywords": [
    "glade",
    "glade-element",
    "annotations",
    "typescript",
    "svelte",
    "web-components"
  ],
  "description": "a component that makes DOM annotatable",
  "version": "2.2.0",
  "dependencies": {
    "@material/mwc-button": "^0.20.0",
    "@material/mwc-dialog": "^0.20.0",
    "@material/mwc-textarea": "^0.20.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.1.1",
    "builtin-modules": "^3.2.0",
    "deepmerge": "^4.2.2",
    "estree-walker": "^1.0.1",
    "firebase": "^8.2.6",
    "fsevents": "^2.3.2",
    "function-bind": "^1.1.1",
    "has": "^1.0.3",
    "is-core-module": "^2.2.0",
    "is-module": "^1.0.0",
    "npm-watch": "^0.7.0",
    "path-parse": "^1.0.6",
    "picomatch": "^2.2.2",
    "require-relative": "^0.8.7",
    "resolve": "^1.19.0",
    "rollup-pluginutils": "^2.8.2",
    "sourcemap-codec": "^1.4.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/glade-software/glade-element"
  },
  "author": "Matt Johnston",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/glade-software/glade-element/issues"
  },
  "homepage": "https://github.com/glade-software/glade-element#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run check",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}