{
  "name": "@glade-software/glade-annotatable",
  "version": "1.0.8",
  "description": "a component that makes DOM annotatable",
  "main": "glade-annotatable.bundled.js",
  "module": "glade-annotatable.js",
  "unpkg": "glade-annotatable.bundled.js",
  "type": "module",
  "files": [
    "glade-annotatable.bundled.js"
  ],
  "scripts": {
    "build": "tsc && rollup -c",
    "build:watch": "tsc --watch && rollup -c -w",
    "clean": "rimraf glade-annotatable.{d.ts,d.ts.map,js,bundled.js,js.map} test/glade-annotatable.{d.ts,d.ts.map,js,js.map} test/glade-annotatable_test.{d.ts,d.ts.map,js,js.map}",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier src/* --write",
    "analyze": "wca analyze \"src/**/*.ts\" --outFile custom-elements.json",
    "prepublishOnly": "npm run build",
    "serve": "es-dev-server --node-resolve --watch",
    "test": "karma start karma.conf.cjs",
    "test:watch": "karma start karma.conf.cjs --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start karma.conf.cjs --update-snapshots",
    "test:prune-snapshots": "karma start karma.conf.cjs --prune-snapshots",
    "checksize": "rollup -c ; cat glade-annotatable.bundled.js | gzip -9 | wc -c ; rm glade-annotatable.bundled.js"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript"
  ],
  "author": "Matt Johnston",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@material/mwc-button": "^0.15.0",
    "@material/mwc-dialog": "^0.15.0",
    "@material/mwc-textarea": "^0.15.0",
    "@material/mwc-textfield": "^0.15.0",
    "firebase": "^7.23.0",
    "lit-element": "^2.4.0"
  },
  "devDependencies": {
    "@open-wc/testing": "^2.5.29",
    "@open-wc/testing-karma": "^4.0.8",
    "@rollup/plugin-replace": "^2.3.2",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "chai": "^4.2.0",
    "deepmerge": "^4.2.2",
    "es-dev-server": "^1.53.0",
    "eslint": "^6.8.0",
    "karma": "^5.1.0",
    "karma-chai": "^0.1.0",
    "karma-mocha": "^2.0.1",
    "lit-analyzer": "^1.1.11",
    "mocha": "^7.2.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^1.32.1",
    "rollup-plugin-filesize": "^7.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^7.0.0",
    "sinon": "^9.0.2",
    "typescript": "^3.9.3",
    "web-component-analyzer": "^1.0.3"
  }
}
